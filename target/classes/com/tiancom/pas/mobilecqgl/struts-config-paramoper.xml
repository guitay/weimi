<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>

	<form-beans>
  	    <form-bean name="mobilecqglForm" type="com.tiancom.pas.mobilecqgl.MobilecqglForm" />
	</form-beans>
	
	<action-mappings>	
  	   <action path="/mobilecqgl"
  	   		   name="mobilecqglForm"
  	   		   attribute="mobilecqglForm"
  	   		   scope="request"
  	   		   input="/../m/cqgl.jsp"
  	   		   parameter="method"
  	   		   validate="true"
  	   		   type="com.tiancom.pas.mobilecqgl.MobilecqglAction">
  	   		<forward name="index" path="/../m/index.jsp" />
  	   		<forward name="ygqd" path="/../m/ygqd.jsp" />
  	   		<forward name="xmlb" path="/../m/xmlb.jsp" />
  	   		<forward name="xmxq" path="/../m/xmxq.jsp" />
  	   		<forward name="lsjl" path="/../m/lsjl.jsp" />
  	   		<forward name="cqgl" path="/../m/cqgl.jsp" />
  	   		<forward name="cqjl" path="/../m/cqjl.jsp" />
  	   		<forward name="cqjlEdit" path="/../m/cqjlEdit.jsp" />
  	   		
  	   		<forward name="fybxmxList" path="/../m/fybxmxList.jsp" />
  	   		<forward name="fybxDpList" path="/../m/fybxDpList.jsp" />
  	   		<forward name="fybxDpmx" path="/../m/fybxDpmx.jsp" />
  	   </action>  
	</action-mappings>
	
	<message-resources parameter="com.tiancom.pas.common.framework.struts.ApplicationResources" />
</struts-config>

